{
  "cmake.sourceDirectory": "${workspaceFolder}/nanodet-wasm",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.codeActionsOnSave": {
      "source.formatOnSave": true,
      "source.organizeImports": true,
      "source.fixAll": true
    }
  },
  "files.associations": {
    "__config": "cpp",
    "random": "cpp",
    "__mutex_base": "cpp",
    "mutex": "cpp",
    "vector": "cpp"
  },
}